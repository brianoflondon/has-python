    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYDEVD_DISABLE_FILE_VALIDATION": "1"
            }
        },
        {
            "name": "has - CLI connect",
            "type": "python",
            "request": "launch",
            "program": "src/has_python/cli/has.py",
            "args": [
                "v4vapp.dev",
                "--key-type",
                "posting",
                // "--token",
                // "bbcb5b87-fcc7-46c1-a760-b4bed22848f9"
            ],
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYDEVD_DISABLE_FILE_VALIDATION": "1"
            }
        },
        {
            "name": "has - CLI brianoflondon",
            "type": "python",
            "request": "launch",
            "program": "src/has_python/cli/has.py",
            "args": [
                "brianoflondon",
                "--key-type",
                "posting",
                // "--token",
                // "bbcb5b87-fcc7-46c1-a760-b4bed22848f9"
            ],
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYDEVD_DISABLE_FILE_VALIDATION": "1"
            }
        },
        {
            "name": "HAS Demo",
            "type": "python",
            "request": "launch",
            "program": "src/run_has.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYDEVD_DISABLE_FILE_VALIDATION": "1"
            }
